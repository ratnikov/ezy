<?xml version="1.0" encoding="UTF-8" ?>
<kml xmlns="http://earth.google.com/kml/2.0"><Response>
  <name><%= @address %></name>
    <Status>
      <code>200</code>
      <request>geocode</request>
    </Status>
    <Placemark id="p1">
      <address><%= @address %></address>
      <Point><coordinates><%= @longitude %>,<%= @latitude %>,0</coordinates></Point>
    </Placemark>
</Response></kml>
